## sshd_config
###########################################################################
[perms=0755] /etc/ssh/sshd_config = {

# Disable legacy (protocol version 1) support in the server for new
# installations. In future the default will change to require explicit
# activation of protocol 1
Protocol 2

HostKey /var/etc/ssh/ssh_host_rsa_key

# Authentication:

LoginGraceTime 600
PermitRootLogin yes

PermitUserEnvironment yes
UseDNS no

# override default of no subsystems
Subsystem	sftp	/usr/libexec/sftp-server
}

###########################################################################
## ssh-start.sh
###########################################################################
[perms=0755] /scripts/ssh-start.sh = {
#!/bin/sh

#This script is intended to run once/boot only, at startup
running=/dev/shmem/ssh-start.run
if [ -e $running ]; then
    echo "$0 Error: SSH script was already run.  Remove $running to re-run this script"
    exit 1;
fi

#Set the umask before creating the SSH files
oldmask=$(umask)
umask 022

#Create the SSH keys
ssh_etcdir="/etc/ssh"
mkdir -p "/var/etc/ssh"
filename="ssh_host_rsa_key"
etclink="/etc/ssh/ssh_host_rsa_key"
varkey="/var/etc/ssh/ssh_host_rsa_key"

if [ ! -f "$etclink" ]; then
    echo "$0 INFO: generating SSH key"
    ssh-keygen -t "rsa" -N '' -f "$varkey" 
fi


#Restore the umask
umask "$oldmask"

#Create the chroot environment
mkdir -p "/var/chroot/sshd/."
chmod 0755 "/var/chroot/sshd"

#Launch sshd
echo "$0 INFO: starting sshd"
/proc/boot/sshd

touch $running
unset running
}

