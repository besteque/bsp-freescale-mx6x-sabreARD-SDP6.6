##**************************************************************************
##			APPSMEDIA-SAMPLE Startup Scripts
##**************************************************************************

[perms=0644] .start-appsmedia-sample = {
	#set env variables
	. /scripts/env.sh

	###########################################################################
	## RAM disk
	###########################################################################
	echo Creating ram disk...
	#By default devb-ram creates an old qnx4 filesystem that only support filenames no more than 48 characters long
	#we need to manually format it to create a new qnx6 filesystem that the maximum filenames is increased to 510 characters
	devb-ram ram capacity=231072 nodinit disk name=ramdisk blk cache=512k qnx6 sync=optional
	waitfor /dev/ramdisk0t77 10
	mkqnx6fs -q /dev/ramdisk0t77
	mount -tqnx6 /dev/ramdisk0t77 /

	echo Ram disk Created.

	#mount the dos partition from SD card to /base.
	#uncomment the mount command if you need a persistent filesystem.
	#make sure your dos partition is located in /dev/hd0t12.

	#mount -tdos -o exe=all /dev/hd0t12 /base 

	#create the diretories, so they are writable
	mkdir -p /bin /sbin /lib/dll/mmedia /etc/ssh /etc/mm /usr/lib/graphics /usr/bin /usr/sbin
	mkdir -p  /var/tmp /var/pps

	echo Starting dumper
	dumper -S -F -d /var/dumper/

	echo Starting PPS
	pps -v -l0 -C -t 300000
	waitfor /pps

	mkdir -p /pps/qnx?nopersist
	mkdir -p /pps/qnx/qdb?nopersist
	echo Starting QDB
	qdb -vv -o verchk=minor
	###########################################################################
	## Network
	###########################################################################
	#create the diretories, so they are writable
	mkdir -p /var/db/ /var/log/net_pps/ /var/run/ /var/etc/system/config/ /pps/services?nopersist /root


	#set current date
	/scripts/setdate.sh &

	###########################################################################
	## Muti-media & usblauncher
	###########################################################################
	/proc/boot/ksh /proc/boot/.start-mmedia
	usblauncher -p 2 -t -s /lib/dll/pubs/

	###########################################################################
	## Browser
	###########################################################################
	#create the diretories, so they are writable
	mkdir -p /pps/services/certmgr?nopersist /var/certmgr/common/user_trusted /var/keymgr/ /var/clipboard/
	mkdir -p /pps/services/launcher?nopersist /accounts/1000/appdata/
	mkdir -p /pps/services/private?nopersist /pps/services/confstr?nopersist
	mkdir -p /pps/services/geolocation?nopersist
	mkdir -p /pps/system/installer/coreos?nopersist /pps/system/installer/registeredapps?nopersist
	mkdir -p /var/dumper /var/fontconfig /etc/id
	mkdir -p /accounts/1000/config /accounts/1000/sharewith /accounts/1000/shared/downloads

	#certmgr
	certmgr_pps &
	waitfor /pps/services/certmgr/control?nopersist

	authman -i -U 505
	cp /proc/boot/launcher /sbin/
	waitfor /sbin/launcher

	/sbin/launcher -i -U 89:0,1000,1004,1100,1104,1200,1204 -m 970 -t 4 -v -b &

	#geolocation services
	ip-provider -E1 > /dev/slog2/debug 2>&1  & 

	#Bar-install the browser app
	echo "INFO: Installing browser app"
	/proc/boot/ksh -c "/scripts/bar-install /appinstall/sys.browser.bar 2>&1 > /dev/slog2/debug"
	/base/bin/webkit-launcher &

	echo "Run the following to launch the browser:"
	echo launch sys.browser

}


