/root/.profile = {
export PATH=/proc/boot:/sbin:/bin:/usr/sbin:/usr/bin:/opt/bin
export LD_LIBRARY_PATH=/proc/boot:/lib:/usr/lib:/lib/dll:/opt/lib
}

###########################################################################
## environment
###########################################################################
[perms=0644]/root/.ssh/environment = {
_=/bin/env
SHELL=/bin/sh
USER=root
MAIL=/var/spool/mail/root
HOME=/root
LOGNAME=root
PATH=/proc/boot:/sbin:/bin:/usr/sbin:/usr/bin:/opt/bin
LD_LIBRARY_PATH=/proc/boot:/lib:/usr/lib:/lib/dll:/opt/lib
ENV=/scripts/env.sh
}

###########################################################################
## launch
###########################################################################
[perms=0755]/sbin/launch = {
#!/bin/sh

if [ "$#" -ne 1 ] || [ "$1" = "" ]
then
	echo "Usage: $0 APP"
	exit 2
fi

APP=$(ls -1 /apps/ | grep $1)

MATCH=$(ls -1 /apps/ | grep -c $1)

if [ "$\{APP\}" = "" ];then
	echo "No such app $1" >&2
	exit 1
fi

if [ $MATCH -gt 1 ];then
	echo "More than one match: $1" >&2
	exit 1
fi

echo "msg::start\\ndat::$\{APP?\},ORIENTATION=0\\nid::$\{APP?\}" > /pps/services/launcher/control

}

###########################################################################
## stop-apps
###########################################################################
[perms=0755]/sbin/stop-apps = {
#!/bin/sh

echo Stopping all running apps ...
echo "msg::stop\\ndat::" > /pps/services/launcher/control
}

